Cell[TextData[{
 "quadratic form of fragment wave functions in ",
 StyleBox["cluster-relative",
  FontWeight->"Bold"],
 " coordinates:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Phi]", "A"], 
     SubscriptBox["\[Phi]", "B"]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["e", 
       RowBox[{"-", 
        RowBox[{
         SubscriptBox["\[Alpha]", "1"], "(", 
         RowBox[{
          RowBox[{"2", " ", 
           RowBox[{
            UnderoverscriptBox["\[Sum]", 
             RowBox[{"j", "=", "1"}], 
             RowBox[{
              SubscriptBox["A", "1"], "-", "1"}]], 
            SubsuperscriptBox["r", "j", "2"]}]}], "+", 
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"j", "\[NotEqual]", "k"}], 
            RowBox[{
             SubscriptBox["A", "1"], "-", "1"}]], 
           SubscriptBox["r", "j"], 
           SubscriptBox["r", "k"]}]}], ")"}]}]], "\[CenterDot]", 
      SuperscriptBox["e", 
       RowBox[{"(", 
        RowBox[{"1", "\[LeftRightArrow]", "2"}], ")"}]]}], "=", 
     RowBox[{
      UnderscriptBox["\[Product]", 
       RowBox[{"i", "\[Element]", 
        RowBox[{"{", 
         RowBox[{"1", ",", "2"}], "}"}]}]], 
      SuperscriptBox["e", 
       RowBox[{
        RowBox[{"-", 
         FractionBox["1", "2"]}], 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["r", "1"], ",", "\[Ellipsis]", ",", 
          SubscriptBox["r", 
           RowBox[{
            SubscriptBox["A", "i"], "-", "1"}]]}], ")"}], 
        RowBox[{"(", 
         RowBox[{GridBox[{
            {
             RowBox[{"4", 
              SubscriptBox["\[Alpha]", "i"]}]},
            {
             RowBox[{"2", 
              SubscriptBox["\[Alpha]", "i"]}]},
            {
             RowBox[{"2", 
              SubscriptBox["\[Alpha]", "i"]}]}
           }], GridBox[{
            {
             RowBox[{"2", 
              SubscriptBox["\[Alpha]", "i"]}]},
            {
             RowBox[{"4", 
              SubscriptBox["\[Alpha]", "i"]}]},
            {
             RowBox[{"2", 
              SubscriptBox["\[Alpha]", "i"]}]}
           }], GridBox[{
            {
             RowBox[{"2", 
              SubscriptBox["\[Alpha]", "i"]}]},
            {
             RowBox[{"2", 
              SubscriptBox["\[Alpha]", "i"]}]},
            {
             RowBox[{"4", 
              SubscriptBox["\[Alpha]", "i"]}]}
           }]}], ")"}], " ", 
