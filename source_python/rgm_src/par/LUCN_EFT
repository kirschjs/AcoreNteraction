      PARAMETER (NZFMAX=570, NZCMAX=8, NZLWMA=22, LWMAX=9,
     *           NZIQMA=9, NZOPER=7, NZPOMA=10, NOUT=6)
      PARAMETER (NDIMC=4400, NDIMCW=4100, NDIMK=81,
     *           NDIMOR=454000)
C
C     DIMENSIONSPARAMETER DER FELDER C, CW, KMV UND
C     DER FELDER DES COMMON-BLOCKS ORD
